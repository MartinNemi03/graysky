{
  "name": "graysky",
  "private": true,
  "engines": {
    "node": ">=v18.16.0"
  },
  "packageManager": "pnpm@8.6.12",
  "scripts": {
    "eas-build-pre-install": "npm install -g pnpm@8.6.12",
    "build": "turbo build",
    "clean": "git clean -xdf node_modules",
    "clean:workspaces": "turbo clean",
    "db:generate": "turbo db:generate",
    "db:push": "turbo db:push db:generate",
    "db:studio": "turbo db:studio",
    "dev:both": "turbo dev --parallel",
    "dev": "pnpm dev:expo",
    "dev:expo": "cd apps/expo && pnpm dev",
    "dev:next": "cd apps/nextjs && pnpm dev",
    "format": "prettier --write \"**/*.{js,cjs,mjs,ts,tsx,md,json}\" --ignore-path .gitignore",
    "lint": "turbo lint && manypkg check",
    "lint:fix": "turbo lint:fix && manypkg fix",
    "type-check": "turbo type-check",
    "postinstall": "pnpm db:generate"
  },
  "dependencies": {
    "@graysky/eslint-config": "^0.1.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.1.0",
    "@manypkg/cli": "^0.21.0",
    "eslint": "^8.47.0",
    "prettier": "^3.0.2",
    "prettier-plugin-tailwindcss": "^0.5.3",
    "turbo": "^1.10.12",
    "typescript": "^5.1.6"
  },
  "overrides": {
    "@tanstack/react-query": "^4.29.12"
  },
  "pnpm": {
    "patchedDependencies": {
      "react-native-svg@13.9.0": "patches/react-native-svg@13.9.0.patch",
      "nativewind@2.0.11": "patches/nativewind@2.0.11.patch"
    }
  }
}
